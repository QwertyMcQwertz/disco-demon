    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
